---
import "../css/styles.css";
import Head from "../components/Head.astro";
import Naviation from "../components/Navigation.astro";
import Footer from "../components/Footer.astro";
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Head />
	<body>
		<Naviation />
		<slot />
		<Footer />
		<script is:inline src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.3/flowbite.min.js"></script>
		<script src="../scripts/main.js"></script>
	</body>
</html>
